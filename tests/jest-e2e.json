{
  "moduleNameMapper": {
    "^src/(.*)": "<rootDir>/src/$1"
  },
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsconfig": {
        "inlineSourceMap": true,
        "target": "esnext"
      }
    }
  },
  "testTimeout": 100000000
}
